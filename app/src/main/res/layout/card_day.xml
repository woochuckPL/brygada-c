<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:id="@+id/cardView"
    android:layout_width="42dp"
    android:layout_height="wrap_content">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#000000"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="1dp"
            android:layout_marginRight="1dp"
            android:orientation="vertical">

            <TextView
                android:id="@+id/cardTxtWeek"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#FFFFFF"
                android:textColor="#000000"
                android:layout_marginBottom="1dp" />

            <TextView
                android:id="@+id/cardTxtMonth"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#FFFFFF"
                android:textColor="#000000"
                android:layout_marginBottom="1dp" />

            <TextView
                android:id="@+id/cardTxtDate"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#FFFFFF"
                android:textColor="#000000"
                android:layout_marginBottom="1dp" />

            <TextView
                android:id="@+id/cardTxtDay"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#FFFFFF"
                android:textColor="#000000"
                android:layout_marginBottom="1dp" />

            <TextView
                android:id="@+id/cardTxtEmp0"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#FFFFFF"
                android:textColor="#000000"
                android:layout_marginBottom="1dp" />

            <TextView
                android:id="@+id/cardTxtEmp1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#FFFFFF"
                android:textColor="#000000"
                android:layout_marginBottom="1dp" />

            <TextView
                android:id="@+id/cardTxtEmp2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#FFFFFF"
                android:textColor="#000000"
                android:layout_marginBottom="1dp" />

            <TextView
                android:id="@+id/cardTxtEmp3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#FFFFFF"
                android:textColor="#000000"
                android:layout_marginBottom="1dp" />

            <TextView
                android:id="@+id/cardTxtEmp4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#FFFFFF"
                android:textColor="#000000"
                android:layout_marginBottom="1dp" />

            <TextView
                android:id="@+id/cardTxtEmp5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#FFFFFF"
                android:textColor="#000000"
                android:layout_marginBottom="1dp" />

            <TextView
                android:id="@+id/cardTxtEmp6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#FFFFFF"
                android:textColor="#000000"
                android:layout_marginBottom="1dp" />

            <TextView
                android:id="@+id/cardTxtEmp7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#FFFFFF"
                android:textColor="#000000"
                android:layout_marginBottom="1dp" />

            <TextView
                android:id="@+id/cardTxtEmp8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#FFFFFF"
                android:textColor="#000000"
                android:layout_marginBottom="1dp" />

            <TextView
                android:id="@+id/cardTxtEmp9"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#FFFFFF"
                android:textColor="#000000"
                android:layout_marginBottom="1dp" />

            <TextView
                android:id="@+id/cardTxtEmp10"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#FFFFFF"
                android:textColor="#000000"
                android:layout_marginBottom="1dp" />

        </LinearLayout>

    </LinearLayout>

</androidx.cardview.widget.CardView>